{
  "name": "@generator-scaffold/storage",
  "version": "0.1.0",
  "description": "Storage package",
  "keywords": [
    "storage",
    "database",
    "interface"
  ],
  "author": "Fabrizio Lallo <fabrizio.lallo95@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ],
    "nyc-arg": [
      "--all",
      "--exclude=**/index.js",
      "--exclude=**/storage.js"
    ],
    "branches": "80",
    "lines": "80",
    "functions": "80",
    "statements": "80",
    "files": "tests/*.test.js"
  },
  "scripts": {
    "format:check": "prettier --check .",
    "docker-compose": "docker-compose up",
    "docker-compose:build": "docker-compose up --build",
    "start": "node lib/server.js",
    "start:dev": "nodemon",
    "test": "TESTING=1 tap"
  },
  "dependencies": {
    "ioredis": "^4.27.10",
    "ioredis-mock": "^5.6.0"
  },
  "engines": {
    "node": ">=12"
  }
}
